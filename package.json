{
  "name": "rollup-plugin-html-string",
  "version": "1.0.1",
  "description": "Rollup plugin to import HTML files as string",
  "type": "module",
  "main": "dist/rollup-plugin-html.js",
  "module": "dist/rollup-plugin-html.mjs",
  "exports": {
    "import": "./dist/rollup-plugin-html.mjs",
    "require": "./dist/rollup-plugin-html.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "html-minifier": "^4.0.0",
    "magic-string": "^0.25.7",
    "rollup-pluginutils": "^2.8.2"
  },
  "devDependencies": {
    "eslint": "7.28.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.23.4",
    "mocha": "8.4.0",
    "rollup": "2.51.2"
  },
  "engines": {
    "node": ">= 12.12.0"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint .",
    "pretest": "pnpm run build",
    "test:js": "mocha test/*.js",
    "test": "pnpm run lint && pnpm run test:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cxres/rollup-plugin-html-string.git"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "html",
    "string"
  ],
  "author": "Rahul Gupta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cxres/rollup-plugin-html-string/issues"
  },
  "homepage": "https://github.com/cxres/rollup-plugin-html-string"
}
